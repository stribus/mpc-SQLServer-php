# Configuração do Banco de Dados SQL Server
# Ajuste os valores conforme seu ambiente

# Configuração para conexão localhost
DB_localhost_host=localhost
DB_localhost_database=master
DB_localhost_integratedSecurity=true

# Se não estiver usando Windows Authentication, descomente e configure:
# DB_localhost_user=seu_usuario
# DB_localhost_password=sua_senha

# Configuração do servidor MCP
MCP_SERVER_NAME=MCP SQL Server
MCP_SERVER_VERSION=1.0.0
MCP_VERSION=2025-06-18

# Debug (opcional)
# PHP_DEBUG=true
